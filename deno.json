{
  "imports": {
    "hono": "jsr:@hono/hono@^4.4.11"
  },
  "tasks": {
    "start": "deno run --allow-net main.ts",
    "start:watch": "deno run --watch --allow-net main.ts"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  },
  "deploy": {
    "exclude": [
      "**/node_modules"
    ],
    "include": [],
    "entrypoint": "main.ts"
  }
}